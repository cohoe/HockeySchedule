[{
    "job": {
        "name": "CBSInteractiveProvider-Tests",
        "scm_provider": "git",
        "scm_url": "https://github.com/cohoe/OpenPuck.git",
        "scm_branch": "master",
        "shell_command": "python scripts/tests/providers/test_Provider.py --seasonids $SEASON_IDS --teamids $TEAM_IDS",
        "parameters": [
            {
                "name": "SEASON_IDS",
                "type": "string",
                "default": "NCAA-1415-W,NCAA-1516-W",
                "description": "A comma-seperated string of Season IDs"
            },
            {
                "name": "TEAM_IDS",
                "type": "string",
                "default": "NCAA-BU-W,NCAA-UConn-W",
                "description": "A comma-seperated string of Team IDs"
            }
        ],
        "wrappers": {
            "inject_env_var": {
                "file": "config/provider_test.properties"
            }
        }
    }
},
